---
import { storyblokEditable } from "@storyblok/astro";

const { blok } = Astro.props;
---

<div class="flex flex-row items-center justify-left gap-4 pt-2">
  <img src={blok.icon.filename} alt={blok.icon.alt} class="opacity-50" />
  <h5
    class="text-white font-Inter text-[14px] sm:text-[16px] font-medium leading-[21px] sm:leading-[21px] tracking-[0em] text-left"
  >
    {blok.contact}
  </h5>
</div>
